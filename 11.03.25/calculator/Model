def plus(num1, num2, show_result_func):
    result = num1 + num2
    show_result_func(result)

def minus(num1, num2, show_result_func):
    result = num1 - num2
    show_result_func(result)

def multiply(num1, num2, show_result_func):
    result = num1 * num2
    show_result_func(result)

def divide(num1, num2, show_result_func):
    result = num1 / num2
    show_result_func(result)
